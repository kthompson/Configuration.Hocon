    <Version>0.0.1</Version>
    <Authors>Kevin Thompson</Authors>
    <PackageId>Configuration.Hocon</PackageId>
    <PackageProjectUrl>https://github.com/kthompson/Configuration.Hocon</PackageProjectUrl>
